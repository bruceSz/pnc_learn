    "import numpy as np\n",
    "d = np.array([\n",
    "    [1,1,1],\n",
    "    [1,0,1],\n",
    "    [0,0,1]\n",
    "])\n",
    "\n",
    "x, y = np.where(d==1)\n",
    "print(\"x is: \" + str(x))\n",
    "print(\"y is : \" + str(y))\n",
    "\n",
    "np.where(d==0)\n",
    "np.stack([x,y],axis=1)"
   ]
